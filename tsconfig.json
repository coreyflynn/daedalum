{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "lib",
    "moduleResolution": "node",
    "importHelpers": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "jsx": "react",
    "lib": ["es2015"]
  },
  "include": ["src/**/*"]
}
